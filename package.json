{
  "name": "express-fileupload-validator",
  "version": "2.0.1",
  "description": "Validator for express-fileupload package",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.ts\"",
    "test": "jest --passWithNoTests",
    "coverage": "jest --passWithNoTests --coverage",
    "publish:lib": "npm test && npm run build && npm publish --access public"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/avivharuzi/express-fileupload-validator.git"
  },
  "keywords": [
    "express",
    "express-fileupload",
    "validator",
    "validation"
  ],
  "author": "Aviv Haruzi <avivharuzi@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/avivharuzi/express-fileupload-validator/issues"
  },
  "homepage": "https://github.com/avivharuzi/express-fileupload-validator#readme",
  "devDependencies": {
    "@types/bytes": "^3.1.0",
    "@types/express-fileupload": "^1.1.6",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.2",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.4",
    "prettier": "^2.3.1",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "tslib": "^2.2.0",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "bytes": "^3.1.0"
  }
}
